  "types": "./build/mod.d.ts",
  "devDependencies": {
    "@swc/cli": "^0.1.45",
    "@swc/core": "^1.2.59",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
