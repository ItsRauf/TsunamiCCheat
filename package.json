    "discord-api-types": "^0.22.0",
    "discord.js": "13.2.0",
    "module-alias": "^2.2.2",
    "mongodb": "^4.1.0",
    "mongoose": "^5.13.15",
    "nodemon": "^2.0.15",
    "octokit": "^1.6.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.2.0",
    "ws": "^8.0.0"
